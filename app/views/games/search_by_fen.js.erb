  const searchResults = document.getElementById("search-results");
  searchResults.innerHTML = "";
};

function populateSearchResults(search_resultsHTML) {
  const searchResults = document.getElementById("search-results");
  searchResults.insertAdjacentHTML('beforeend', searchResultsHTML);
};

